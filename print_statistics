#### print_statistics ####def print_statistics(a1, a2):    sta1 = scs.describe(a1)    sta2 = scs.describe(a2)    print ('size', sta1[0], sta2[0])    print ('min', sta1[1][0], sta2[1][0])    print ('max', sta1[1][1], sta2[1][1])    print ('mean', sta1[2], sta2[2])    print ('std', np.sqrt(sta1[3]), np.sqrt(sta2[3]))    print ('skew', sta1[4], sta2[4])    print ('kurtosis', sta1[5], sta2[5])